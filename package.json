{
  "name": "demos",
  "version": "1.0.0",
  "description": "demo application to show how to structure a front-end app",
  "main": "bundle.js",
  "dependencies": {
    "angular": "^1.5.8",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "backbone": "^1.3.3",
    "backbone-events-standalone": "^0.2.7",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.23.1",
    "fastdom": "^1.0.4",
    "file-loader": "^0.9.0",
    "flexboxgrid": "^6.3.1",
    "html-loader": "^0.4.3",
    "image-loader": "^0.0.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "mezr": "github:niklasramo/mezr",
    "ngtemplate-loader": "^1.3.1",
    "normalize.css": "^4.2.0",
    "onchange": "^3.0.2",
    "postcss-loader": "^0.9.1",
    "pug": "^2.0.0-beta5",
    "pug-loader": "^2.2.1",
    "react": "^15.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "cson-loader": "^0.1.0"
  },
  "config": {
    "onchange": "onchange 'src/**/*.pug' 'build/components/static/*.js' -i --",
    "index": "pug src/bootstrap-app/page/index.pug -o ./ -f",
    "entry": "node build-website.js > build/components/site/build.html",
    "bootstrap": "node-sass --output-style compressed src/bootstrap-app/custom-bootstrap-grid/bootstrap.scss -o build/bootstrap-app/custom-bootstrap-grid/",
    "client": "node-sass --output-style compressed src/components/site/build.sass -o build/components/site/"
  },
  "scripts": {
    "clean": "rm -rf build",
    "bPUG:index": "eval \"$npm_package_config_index\"",
    "bPUG:entry": "eval \"$npm_package_config_entry\"",
    "bPUG": "npm run bPUG:entry && npm run bPUG:index",
    "bES6:client": "babel src -d build",
    "bSASS:client": "eval \"$npm_package_config_client\"",
    "bSASS": "npm run bSASS:client",
    "b0": "npm run bES6:client && npm run bSASS && npm run bPUG",
    "wPUG:index": "eval \"$npm_package_config_index -w &\"",
    "wPUG:entry": "eval \"$npm_package_config_onchange $npm_package_config_entry &\"",
    "wPUG": "npm run wPUG:index && npm run wPUG:entry",
    "wES6:client": "babel -w src/ -d build/ &",
    "wSASS:client": "eval \"$npm_package_config_client -w &\"",
    "wSASS": "npm run wSASS:client",
    "w": "npm run b0 && npm run wES6:client && npm run wSASS && npm run wPUG",
    "dev": "npm run clean && npm run w && webpack-dev-server --inline --hot",
    "build": "npm run clean && npm run b0 && webpack --optimize-minimize",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC"
}
